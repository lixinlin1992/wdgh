<?xml version='1.0' encoding='UTF-8'?>
<query-config>
    <query id="Q_LOAD_yftkactivityintroduction" type="pagelist" result="easyui">
        <stmt><![CDATA[
		select t.activity_id,t.sponsor,t.subject,t.subject_pic,t.activity_time,
		t.contact,t.fee,t.sum_limit,t.address,t.content,t.start_time,t.end_time
		from ${table_name} t
		where t.activity_id=[id]
        ]]></stmt>
    </query>
</query-config>