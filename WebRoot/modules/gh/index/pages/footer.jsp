        mySwiper4.swipeNext()
    });

	//修改页脚类别为动态展示 xyj 2017/8/5
    var INDEX_MENUS;
    rdcp.ready(function(){
        loadFooterMenus();
    });
    function loadFooterMenus(){
        rdcp.request("!gh/index/~query/Q_INDEX_MENU_LIST",{},function(data){
            INDEX_MENUS = data.body.rows;
            for(var i=0;i<INDEX_MENUS.length;i++){
                var html = "";
                var t = INDEX_MENUS[i];
                if(t.MENU_TYPE=="1") {
                    html += "<a href='"+t.MENU_URL+"'>" + t.MENU_NAME + "</a>&nbsp;&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;&nbsp;";
                    $(".g-footer-cat-left").append(html);
                }
            }
        });
    }
</script>